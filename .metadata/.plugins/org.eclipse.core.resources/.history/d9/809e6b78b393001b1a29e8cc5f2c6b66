package beginner;

import java.util.Scanner;

public class TimesTable1 {

	public static void main(String[] args) {

		System.out.print("출력하고 싶은 구구단의 시작 범위와 끝 범위를 입력하세요: ");
		Scanner scan = new Scanner(System.in);
		int s = scan.nextInt();
		int e = scan.nextInt();
		
		if(s < e) {
			for(int i=0; i<9; i++) {
				for(int j=s; j<e+1; j++) {
					if(j*(i+1) % 10 == j*(i+1))
						System.out.printf("%d * %d =  %d   ", j, i+1, j*(i+1));
					else 
						System.out.printf("%d * %d = %d   ", j, i+1, j*(i+1));
				}
				
				System.out.println();
			}
		} else {
			for(int i=0; i<9; i++) {
				for(int j=s; j>e-1; j--) {
					if(j*(i+1) % 10 == j*(i+1))
						System.out.printf("%d * %d =  %d   ", j, i+1, j*(i+1));
					
					System.out.printf("%d * %d = %d   ", j, i+1, j*(i+1));
				}
				
				System.out.println();
			}
		}
		
	}

}
