package beginner;

import java.util.Scanner;

public class CharNumber2 {

	public static void main(String[] args) {

		System.out.print("정사각형 한 변의 길이 n을 입력하세요: ");
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		int[][] nums = new int[n][n];
		
		System.out.printf("%c", 'A'+ 1);
		
	}

}
