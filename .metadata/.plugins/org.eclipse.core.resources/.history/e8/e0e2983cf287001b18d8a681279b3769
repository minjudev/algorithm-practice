package beginner;

import java.util.Scanner;

public class NumberSquare1 {

	public static void main(String[] args) {

		System.out.print("사각형의 높이 n과 너비 m을 순서대로 입력하세요: ");
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		int m = scan.nextInt();
		
		// n행 m열의 이차원 배열을 생성한다.
		// 1행 1열은 1로 시작한다.
		// for문을 돌며 n행 안에서 오른쪽으로 지나가며 1씩 커진다.
		// 이것을 m번 반복한다.
		// for문을 다 돌았으면 한 줄 띈다.
		// 이것을 n번 반복한다.
		
		int[][] nums = new int[n][m];
		int startNum = 0;
		
		for(int i=0; i<n; i++) {
			for(int j=0; j<m; j++) {
				startNum++;
				
				System.out.printf("%d ", startNum);
			}
			
			System.out.println();
		}
		
	}

}
